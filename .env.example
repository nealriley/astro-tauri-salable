# Salable Beta API Key
# Get your API key from https://beta.salable.app
SALABLE_API_KEY=your_salable_api_key_here

# Salable Plan IDs
# Create these in your Salable dashboard under Products > Plans
SALABLE_PLAN_FREE=your_free_plan_id
SALABLE_PLAN_PRO=your_pro_plan_id
SALABLE_PLAN_BUSINESS=your_business_plan_id

# Demo username (used by Tauri to simulate granteeId)
DEMO_USERNAME=demo-user-123

# Tauri Dev Host (used by tauri:dev:headless script)
# Set this to the hostname where your Astro server is running
TAURI_DEV_HOST=localhost
